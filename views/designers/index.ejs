<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

 <h1><%=  title  %></h1>


 <table id="list">
    <thead>
      <tr>
        <th>Name</th>
        <th>@</th>
      </tr>
    </thead>
    <tbody>
      <% designers.forEach((d) => { %>
      <tr>
        <td><%= d.name %></td>
        <td><%= d.userName %></td>
        <td><a href="/designers/<%= d._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>

<a href='/'>HOME</a>

  </table>
  </body>
</html>